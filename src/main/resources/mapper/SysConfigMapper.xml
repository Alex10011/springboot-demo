<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.alex10011.example.dao.SysConfigDao">
	
	<select id="queryAll" resultType="com.alex10011.example.domain.SysConfig">
		SELECT
